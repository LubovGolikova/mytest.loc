{% extends 'base.html.twig' %}
{% block header %}
    <header>
        <div class="d-flex flex-column ">
            <h1 class="title-events"> All user </h1>
        </div>
    </header>
{% endblock %}
{% block body %}
    {% for user in users %}
        <div class="container-events container">
            <h2>{{  user.username}}</h2>
            <h2>{{  user.password}}</h2>
            <h2>{{  user.email}}</h2>

        </div>
    {% endfor %}
{% endblock %}
